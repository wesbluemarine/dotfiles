#!/bin/bash
~/.local/bin/slstatus/slstatus &
~/.local/bin/xvisbell/xvisbell &
hsetroot -solid "#f2e5bc" &
picom --config ~/.config/picom/picom.conf &
redshift &
xbanish &
dbus-launch dunst &

while true; do
# Log stderror to a file
  ~/.local/bin/dwm-6.2/dwm 2> ~/.dwm.log
#No error logging
  #~/.local/bin/dwm-6.2/dwm >/dev/null 2>&1
done
